{
  "name": "git-tools",
  "main": "./lib/git-tools",
  "version": "0.4.1",
  "description": "Opens Git Gui and gitk",
  "keywords": [
    "git",
    "gitk",
    "git-gui"
  ],
  "activationCommands": {
    "atom-workspace": [
      "git-tools:git-gui",
      "git-tools:git-gui-blame",
      "git-tools:gitk",
      "git-tools:gitk-current-file"
    ]
  },
  "repository": "https://github.com/ankalus/git-tools.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "configSchema": {
    "blameCursorNotification": {
      "title": "Show notification of used line if multiple cursors are present",
      "description": "This will display a notification of the used line for the \"Git Gui Blame\"-command.",
      "type": "boolean",
      "default": true,
      "order": 1
    }
  }
}
